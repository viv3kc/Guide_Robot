<launch>
	<master auto="start"/>
	<node
		pkg="tf"
		type="static_transform_publisher"
		name="map_to_camera"
		args="0 0 0 0 0 0 /base_link /camera_link 100"
		/>
	<node
		pkg="move_base"
		type="move_base"
		respawn="false"
		name="move_base"
		output="screen"
	>
		<rosparam
			file="$(find robot_guide)/2d_nav/robot_params/costmap_common_params.yaml"
			command="load"
			ns="global_costmap"/>
		<rosparam
			file="$(find robot_guide)/2d_nav/robot_params/costmap_common_params.yaml"
			command="load"
			ns="local_costmap"/>
		<rosparam
			file="$(find robot_guide)/2d_nav/robot_params/local_costmap_params.yaml"
			command="load"/>
		<rosparam
			file="$(find robot_guide)/2d_nav/robot_params/global_costmap_params.yaml"
			command="load"/>
		<rosparam
			file="$(find robot_guide)/2d_nav/robot_params/base_local_planner_params.yaml"
			command="load"/>
		<rosparam
			file="$(find robot_guide)/2d_nav/stage_params/global_planner_params.yaml"
			command="load"/>
        <rosparam
			file="$(find robot_guide)/2d_nav/stage_params/move_base_params.yaml"
			command="load"/>
	</node>
  	<node
		pkg="map_server"
		name="map_server"
  		type="map_server"
  		args="$(find robot_guide)/map/newlowerground.yaml"
	/>
	<node
		name="amcl"
		pkg="amcl"
		type="amcl"
		args="scan:=base_scan"
	/>
</launch>
